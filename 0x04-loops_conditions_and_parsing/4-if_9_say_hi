#!/usr/bin/env bash
# This displays "Best School" 10 times using a while loop.
# On the 9th iteration, display "Hi" before "Best School".

count=1

while [ $count -le 10 ]; do
    if [ $count -eq 9 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    ((count++))
done
